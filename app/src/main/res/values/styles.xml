<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="myTheme1" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowContentTransitions">true</item>
        <!-- specify shared element transitions -->
        <item name="android:windowSharedElementEnterTransition">
            @transition/changebounds</item>
        <item name="android:colorPrimaryDark" tools:ignore="PrivateResource">@color/primary_dark_material_light</item>
        <item name="android:colorBackground">@color/add_customer_background</item>
        <item name="android:statusBarColor">@color/app_green</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:fontFamily">@font/heebo_light</item>
    </style>
    <style name="myTheme1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColorHint">@android:color/black</item>
    </style>
</resources>
